{
  "parser": "@typescript-eslint/parser",
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
